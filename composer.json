{
    "name": "mutation/imagesoptim",
    "description": "Optimize images",
    "type": "craft-plugin",
    "support": {
        "docs": "https://github.com/MutationDigitale/craft3-imagesoptim/blob/master/README.md",
        "issues": "https://github.com/MutationDigitale/craft3-imagesoptim/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mutation Digitale",
            "homepage": "https://mutation.io/"
        }
    ],
    "require": {
        "php": ">7.1",
        "craftcms/cms": "^3.0.0",
        "spatie/image-optimizer": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "mutation\\imagesoptim\\": "src/"
        }
    },
    "extra": {
        "name": "Images optimizations",
        "handle": "imagesoptim",
        "hasCpSettings": false,
        "hasCpSection": false,
        "schemaVersion": "1.0.0",
        "changelogUrl": "https://raw.githubusercontent.com/MutationDigitale/craft3-imagesoptim/master/CHANGELOG.md",
        "class": "mutation\\imagesoptim\\ImagesOptimPlugin"
    }
}
